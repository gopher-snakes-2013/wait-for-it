<div class="nav">
<%= render "restaurants/logout" %>
</div>
<br/>
<div class="create-form">
  <%= render "reservations/form" %>
</div>

<div class="error-message">
  <%= flash[:error] %>
</div>

<div class="table">
  <div class="table-body">
    <% @reservations.each do |reservation| %>
      <div class="each-reservation">
        <%= render partial: "reservations/show", locals: { restaurant: @restaurant, reservation: reservation } %>
      </div>
    <% end %>
  </div>
</div>

